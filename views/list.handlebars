<div class="box-contianer">
    <div class="section holder">
        <div class="row" id="rowOne">
            {{#each inventory}}
            <div class="col s6" style=" border-radius: 5px; width: 230px">
                <div class="item-container">
                    <div class="item-pic">
                        <a href="/toy/{{id}}/{{UserId}}" class="opa-hover">
                            <img class="vertical-middle" width="200" height="160" src="{{url}}" alt="{{title}}" style="margin-top: 10px">
                        </a>
                    </div>
                    
                    <div class="item-info">
                        <div class="item-info-title" style="text-align: center; font-family: Roboto, sans-serif; color: black">
                            <a href="/toy/{{id}}">{{title}}</a>
                        </div>
                        <div class="item-info-price" style="text-align: right; font-size: 20px; color: #969696; font-family: Source Sans Pro,Helvetica Neue,Helvetica,Arial,sans-serif; margin-right: 10px">${{price}}</div>
                        
                        <div class="item-info-condition" style="text-align: right; font-size: 12p; margin-right:10px">
                            <span>{{product_condition}}</span></div>
                    </div>
                    <a href="/toy/{{id}}" class="bold">
                        <div class="owner-info" style="border-top: 1px solid rgb(216, 216, 216); width: 200px; margin-bottom: 10px">
                            <div class="owner-info-pic">
                                {{!-- <img class="avatar vertical-middle" src="https://vignette.wikia.nocookie.net/logopedia/images/4/4c/Walmart-logo-new.jpg/revision/latest/scale-to-width-down/639?cb=20130919205844g"
                                    width="100" height="30" style="margin-left: 55px">
                                     --}}


                            </div>
                          
                            <div class="col s2">
                                <i class="material-icons  teal-text" style="margin-left:100px">email star local_offer</i>
                                
                            </div>
                        </div>
                    </a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>

