    {{#each toy}}
    <div class="row">
        <br>
        <div class="col m12 text-center">
            <div>
                <div class="row">
                    <div class="col m6">
                        <div id='imgCont'>
                            <img src="{{url}}" class='viewImg' alt="Img not found.">
                        </div>
                        <div>
                            <div>
                                <i class="material-icons">star</i>
                                <span id="text-one">Add to Watch list</span>
                            </div>
                            <span id="text-two">5 people watching</span>
                        </div>
                    </div>
                    <div class="col m4 text-left">
                        <div class="col m-2">
                            <div class="pl-2 pt-3">
                                <span id="title">{{title}}</span>
                            </div>
                            <hr>
                            <div class="m-3">
                                <!-- Modal -->
                                <div id="myModal" class="modal">
                                    <div class="modal-content">
                                        <span class="close">&times;</span>
                                        <p id='modalPMsg'></p>
                                        <div id='allModalBtns'>
                                            <button class="btn btn-success" id='modalOfferBtn'>Offer</button>
                                            <button class="btn btn-danger" id='modalCancelBtn'>Cancel</button>
                                        </div>
                                    </div>
                                </div>
                                <!-- ******** -->
                                
                                <div class="d-inline-block ml-2">
                                    <div class="text-right">This item is {{product_condition}}</div>
                                    <div class="text-right">Additional Info:</div>
                                    <div class="text-right">{{availability}} in stock</div>
                                    <div class="text-right">Suggested Retail Price: {{price}}</div>
                                </div>
                                <div class="d-inline-block">
                                </div>
                            </div>
                            <div id="dataContBot" class="pr-3">
                                <div class="ml-3 mt-2">
                                    <div class="pt-2 font-weight-bold">Owner Contact Information:</div>
                                    <hr>
                                    <div class="d-inline-block">
                                        <div class="text-right">Username: {{User.username}}</div>
                                        {{!-- <div class="text-right">City, State:</div> --}}
                                        <div class="text-right">Email: {{User.email}}</div>
                                    </div>
                                    {{!-- <div class="d-inline-block mb-3">
                                        <div class="text-left ml-2">John Doe</div>
                                        <div class="text-left ml-2">Newark, NJ</div>
                                        <div class="text-left ml-2">johndoe@gmail.com</div>
                                    </div> --}}
                                    <div class="mb-3">
                                        <div class="row pb-2">
                                            <div class="col-md-6">
                                                <button class="my-1 btn btn-success" id="tradeBtn">Offer Trade</button>
                                                <button class="my-1 btn btn-info" data-toggle='tooltip' title='Not Available for chat.' data-placement="top" id="chatBtn">Chat</button>
                                            </div>
                                            <div class="col m-6">
                                                <a href="#">
                                                        <img src="/assets/img/questionIcon.png" id="qIcon" alt="Img not found." class="mt-2">
                                                        <span>Ask a Question</span>
                                                </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row mt-3">
            <div class="col m6 text-center">
                <fieldset>
                    <legend id="idLegend">Description/Additional Info</legend>
                    <p>{{description}}</p>
                </fieldset>
            </div>
            <div class="col m6 text-center">
                <div id='chatDiv' class="m3 m1">
                    <div id="topDiv">
                        <div id="msgDiv" class="m4 m3 p1 m1 text-left"></div>
                    </div>
                    <div id="bottDiv"></div>
                </div>
            </div>
        </div>
    </div>
    {{/each}}
    <div class="productsFromUser">
                    {{#each inventory}}
            <div class="col s6" style="border: 1px solid lightgrey; border-radius: 5px; width: 230px">
                <div class="item-container">
                    <div class="item-pic">
                        <a href="/toy/{{id}}/{{UserId}}" class="opa-hover">
                            <img class="vertical-middle" width="150" height="150" src="{{url}}" alt="{{title}}" style="margin-top: 10px">
                        </a>
                    </div>
                    <div class="item-info">
                        <div class="item-info-title">
                            <a href="/toy/{{id}}">{{title}}</a>
                        </div>
                        <div class="item-info-price">${{price}}</div>
                        <div class="item-info-distance">
                            <i class="ss-location"></i>{{availability}} in stock
                        </div>
                        <div class="item-info-condition">{{product_condition}}</div>
                    </div>
                    <a href="/toy/{{id}}" class="bold">
                        <div class="owner-info" style="border-top: 1px solid rgb(216, 216, 216); width: 200px; margin-bottom: 10px">
                            <div class="owner-info-pic">
                                <img class="avatar vertical-middle" src="https://d2j6tswx2otu6e.cloudfront.net/p-3dXNhNt7gDC1qs2dwNcLYM0mQ=/100x100/smart/744b/o29534662_49196.jpg"
                                    width="30" height="30" alt=" ">
                            </div>
                            Username : {{User.username}}
                            <div class="col s2">
                                <a href="#">
                                    <i class="material-icons" style="border: 1px solid lightgrey; border-radius: 2px">share</i>
                                </a>
                                <a href="#">
                                    <i class="material-icons" style="border: 1px solid lightgrey; border-radius: 2px">favorite_border</i>
                                </a>
                            </div>
                        </div>
                    </a>
                </div>
            </div>
            {{/each}}
    </div>
